<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.intranet2">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name="intranet.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="intranet.views.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="intranet.views.UserActivity" />
        <activity android:name="intranet.views.AddStudentActivity" />
        <activity android:name="intranet.views.ProfileActivity" />
        <activity android:name="intranet.views.Splash" />
        <activity android:name="intranet.views.AddTeacherActivity" />
        <activity android:name="intranet.views.SignUpActivity"></activity>
    </application>

</manifest>